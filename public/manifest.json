{
  "manifest_version": 3,
  "name": "Todo Extension",
  "version": "1.0.0",
  "description": "A simple Todo Chrome extension built with React",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  }
}
